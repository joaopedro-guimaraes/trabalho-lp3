      </GridPane>
      <Group layoutX="155.0" layoutY="112.0" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <children>
            <Button fx:id="buttonCancelar" layoutX="107.0" mnemonicParsing="false" onAction="#clickButtonCancelar" text="Cancelar" />
